{% extends "base.html.twig" %}
{% block title %}{% trans %} About Us {% endtrans %} | STG Mining Supplies{% endblock %}
{% block content %}
<section class="top-space-margin page-title-big-typography cover-background pt-0 pb-0" style="background-image: url(/images/mining_slider_0002.jpg); margin-top: 135px;">
<div class="container">
<div class="row align-items-center justify-content-center small-screen">
<div class="col-xl-5 col-sm-8 page-title-double-large text-center" data-anime="{ &quot;el&quot;: &quot;childs&quot;, &quot;opacity&quot;: [0, 1], &quot;translateY&quot;: [30, 0], &quot;duration&quot;: 600, &quot;staggervalue&quot;: 300, &quot;easing&quot;: &quot;easeOutQuad&quot; }">
<h1 class="text-base-color fw-700 mb-5px" style="">{% trans %}About us{% endtrans %}</h1>
</div>
</div>
</div>
</section>

<section class="position-relative overflow-hidden overlap-height pb-0">
<img src="/files/mining.about-02.png" style = "left: 250px !Important;" class="position-absolute left-150px top-50px z-index-minus-1 skrollable skrollable-before" alt="" data-bottom-top="transform: translate3d(80px, 0px, 0px);" data-top-bottom="transform: translate3d(-380px, 0px, 0px);" style="transform: translate3d(80px, 0px, 0px);" data-no-retina="">
